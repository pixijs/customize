{
    "packages": [
        {
            "name": "@pixi/accessibility",
            "namespace": "accessibility",
            "rendererPlugin": ["accessibility", "AccessibilityManager"],
            "canvasPlugin": ["accessibility", "AccessibilityManager"]
        },
        {
            "name": "@pixi/app"
        },
        {
            "name": "@pixi/constants",
            "required": true
        },
        {
            "name": "@pixi/core",
            "required": true,
            "rendererPlugin": ["batch", "BatchRenderer"]
        },
        {
            "name": "@pixi/display",
            "required": true
        },
        {
            "name": "@pixi/extract",
            "namespace": "extract",
            "rendererPlugin": ["extract", "Extract"]
        },
        {
            "name": "@pixi/graphics"
        },
        {
            "name": "@pixi/interaction",
            "namespace": "interaction",
            "rendererPlugin": ["interaction", "InteractionManager"]
        },
        {
            "name": "@pixi/loaders",
            "appPlugin": "AppLoaderPlugin"
        },
        {
            "name": "@pixi/math",
            "required": true
        },
        {
            "name": "@pixi/mesh"
        },
        {
            "name": "@pixi/mesh-extras"
        },
        {
            "name": "@pixi/mixin-cache-as-bitmap",
            "importOnly": true
        },
        {
            "name": "@pixi/mixin-get-child-by-name",
            "importOnly": true
        },
        {
            "name": "@pixi/mixin-get-global-position",
            "importOnly": true
        },
        {
            "name": "@pixi/particles",
            "rendererPlugin": ["particle", "ParticleRenderer"]
        },
        {
            "name": "@pixi/polyfill",
            "importOnly": true
        },
        {
            "name": "@pixi/prepare",
            "namespace": "prepare",
            "rendererPlugin": ["prepare", "Prepare"]
        },
        {
            "name": "@pixi/runner",
            "required": true
        },
        {
            "name": "@pixi/settings",
            "required": true
        },
        {
            "name": "@pixi/sprite"
        },
        {
            "name": "@pixi/sprite-animated"
        },
        {
            "name": "@pixi/sprite-tiling",
            "rendererPlugin": ["tilingSprite", "TilingSpriteRenderer"]
        },
        {
            "name": "@pixi/spritesheet",
            "loaderPlugin": "SpritesheetLoader"
        },
        {
            "name": "@pixi/text"
        },
        {
            "name": "@pixi/text-bitmap",
            "loaderPlugin": "BitmapFontLoader"
        },
        {
            "name": "@pixi/ticker",
            "required": true,
            "appPlugin": "TickerPlugin"
        },
        {
            "name": "@pixi/unsafe-eval",
            "code": [
                "import * as PIXI from '@pixi/core'",
                "import { install } from '${name}'",
                "install(PIXI)"
            ]
        },
        {
            "name": "@pixi/utils",
            "namespace": "utils",
            "required": true
        },
        {
            "name": "@pixi/filter-alpha",
            "filter": ["AlphaFilter"]
        },
        {
            "name": "@pixi/filter-blur",
            "filter": ["BlurFilter", "BlurFilterPass"]
        },
        {
            "name": "@pixi/filter-color-matrix",
            "filter": ["ColorMatrixFilter"]
        },
        {
            "name": "@pixi/filter-displacement",
            "filter": ["DisplacementFilter"]
        },
        {
            "name": "@pixi/filter-fxaa",
            "filter": ["FXAAFilter"]
        },
        {
            "name": "@pixi/filter-noise",
            "filter": ["NoiseFilter"]
        },
        {
            "name": "@pixi/canvas-display",
            "importOnly": true
        },
        {
            "name": "@pixi/canvas-extract",
            "namespace": "extract",
            "canvasPlugin": ["extract", "CanvasExtract"],
            "code": [
                "import * as canvasExtract from '${name}'",
                "Object.assign(${namespace}, canvasExtract)"
            ]
        },
        {
            "name": "@pixi/canvas-graphics",
            "canvasPlugin": ["graphics", "CanvasGraphicsRenderer"]
        },
        {
            "name": "@pixi/canvas-mesh",
            "canvasPlugin": ["mesh", "CanvasMeshRenderer"]
        },
        {
            "name": "@pixi/canvas-particles",
            "importOnly": true
        },
        {
            "name": "@pixi/canvas-prepare",
            "namespace": "prepare",
            "canvasPlugin": ["prepare", "CanvasPrepare"],
            "code": [
                "import * as canvasPrepare from '${name}'",
                "Object.assign(prepare, canvasPrepare)"
            ]
        },
        {
            "name": "@pixi/canvas-renderer"
        },
        {
            "name": "@pixi/canvas-sprite",
            "canvasPlugin": ["prepare", "CanvasSpriteRenderer"]
        },
        {
            "name": "@pixi/canvas-sprite-tiling",
            "importOnly": true
        }
    ],
    "order": [
        "@pixi/polyfill",
        "@pixi/constants",
        "@pixi/math",
        "@pixi/runner",
        "@pixi/settings",
        "@pixi/ticker",
        "@pixi/utils",
        "@pixi/display",
        "@pixi/core",
        "@pixi/unsafe-eval",
        "@pixi/mixin-get-child-by-name",
        "@pixi/mixin-get-global-position",
        "@pixi/canvas-display",
        "@pixi/extract",
        "@pixi/loaders",
        "@pixi/mesh",
        "@pixi/particles",
        "@pixi/sprite",
        "@pixi/canvas-extract",
        "@pixi/canvas-renderer",
        "@pixi/filter-alpha",
        "@pixi/filter-blur",
        "@pixi/filter-color-matrix",
        "@pixi/filter-displacement",
        "@pixi/filter-fxaa",
        "@pixi/filter-noise",
        "@pixi/accessibility",
        "@pixi/app",
        "@pixi/graphics",
        "@pixi/mesh-extras",
        "@pixi/mixin-cache-as-bitmap",
        "@pixi/sprite-animated",
        "@pixi/sprite-tiling",
        "@pixi/spritesheet",
        "@pixi/canvas-particles",
        "@pixi/canvas-sprite",
        "@pixi/text-bitmap",
        "@pixi/text",
        "@pixi/canvas-graphics",
        "@pixi/canvas-mesh",
        "@pixi/canvas-sprite-tiling",
        "@pixi/interaction",
        "@pixi/prepare",
        "@pixi/canvas-prepare"
    ],
    "groups": [
        {
            "title": "Core",
            "packages": [
                "@pixi/accessibility",
                "@pixi/app",
                "@pixi/constants",
                "@pixi/core",
                "@pixi/display",
                "@pixi/extract",
                "@pixi/graphics",
                "@pixi/interaction",
                "@pixi/loaders",
                "@pixi/math",
                "@pixi/mesh",
                "@pixi/mesh-extras",
                "@pixi/mixin-cache-as-bitmap",
                "@pixi/mixin-get-child-by-name",
                "@pixi/mixin-get-global-position",
                "@pixi/particles",
                "@pixi/polyfill",
                "@pixi/prepare",
                "@pixi/runner",
                "@pixi/settings",
                "@pixi/sprite",
                "@pixi/sprite-animated",
                "@pixi/sprite-tiling",
                "@pixi/spritesheet",
                "@pixi/text",
                "@pixi/text-bitmap",
                "@pixi/ticker",
                "@pixi/unsafe-eval",
                "@pixi/utils"
            ]
        },
        {
            "title": "Filters",
            "packages": [
                "@pixi/filter-alpha",
                "@pixi/filter-blur",
                "@pixi/filter-color-matrix",
                "@pixi/filter-displacement",
                "@pixi/filter-fxaa",
                "@pixi/filter-noise"
            ]
        },
        {
            "title": "Canvas",
            "packages": [
                "@pixi/canvas-display",
                "@pixi/canvas-extract",
                "@pixi/canvas-graphics",
                "@pixi/canvas-mesh",
                "@pixi/canvas-particles",
                "@pixi/canvas-prepare",
                "@pixi/canvas-renderer",
                "@pixi/canvas-sprite",
                "@pixi/canvas-sprite-tiling"
            ]
        }
    ]
}
